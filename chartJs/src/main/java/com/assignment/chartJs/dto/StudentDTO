package com.assignment.chartJs.dto;

import com.fasterxml.jackson.databind.annotation.JsonSerialize;

@JsonSerialize
public class StudentDTO {
	
	private int seq;
    private String name;
    private String gender;
    private Byte level;
    private Byte track;
    private String title;
    private Byte value;

    
    public StudentDTO (int seq, String name, String gender, Byte level, Byte track, String title, Byte value) {
        this.seq = seq;
        this.name = name;
        this.gender = gender;
        this.level = level;
        this.track = track;
        this.title = title;
        this.value = value;
        
    }

}
